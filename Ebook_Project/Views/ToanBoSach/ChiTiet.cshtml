        const stars = document.querySelectorAll('.stars a');
        stars.forEach(star => {
            star.classList.remove('text-yellow-500');
        });

        // Highlight the clicked star
        const clickedStar = event.target;
        clickedStar.classList.add('text-yellow-500');
    }
</script>
<script>
    document.addEventListener('DOMContentLoaded', function () {
        // Automatically show content for the "Mô tả" tab and add styles
        var descriptionTabButton = document.querySelector('.tablinks.active');
        descriptionTabButton.classList.add('focus:border-t', 'focus:border-l', 'focus:border-r', 'focus:border-gray-300', 'font-bold');
        openTab({ currentTarget: descriptionTabButton }, 'Description');
    });

    function openTab(event, tabName) {
        // Get all elements with class="tabcontent" and hide them
        var tabcontent = document.getElementsByClassName('tabcontent');
        for (var i = 0; i < tabcontent.length; i++) {
            tabcontent[i].style.display = 'none';
        }

        // Get all elements with class="tablinks" and remove the class "active"
        var tablinks = document.getElementsByClassName('tablinks');
        for (var i = 0; i < tablinks.length; i++) {
            tablinks[i].classList.remove('active', 'font-bold');
            // Remove focus styles for all tabs
            tablinks[i].classList.remove('focus:border-t', 'focus:border-l', 'focus:border-r', 'focus:border-gray-300');
        }

        // Show the current tab, and add an "active" class to the button that opened the tab
        document.getElementById(tabName).style.display = 'block';
        event.currentTarget.classList.add('active', 'font-bold');
        // Add focus styles for the active tab
        event.currentTarget.classList.add('focus:border-t', 'focus:border-l', 'focus:border-r', 'focus:border-gray-300');
    }
</script>

